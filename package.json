{
  "name": "cssqueeze",
  "version": "1.0.9",
  "description": "A CLI tool to optimize CSS files",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && babel src --out-dir build --copy-files",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/calvinkarundu/cssqueeze.git"
  },
  "keywords": [
    "css",
    "minify",
    "prefix",
    "import",
    "postcss"
  ],
  "author": "Calvin Karundu <calvin.mwenda@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calvinkarundu/cssqueeze/issues"
  },
  "homepage": "https://github.com/calvinkarundu/cssqueeze#readme",
  "preferGlobal": true,
  "files": [
    "build"
  ],
  "bin": {
    "cssqueeze": "build/bin/index.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "commander": "^2.11.0",
    "cssnano": "^4.0.0-rc.2",
    "cssnano-preset-advanced": "^4.0.0-rc.2",
    "postcss": "^6.0.11",
    "postcss-import": "^10.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0"
  }
}
